				<property>
					<name>generateUml</name>
				</property>
			</activation>
			<build>
				<pluginManagement>
					<plugins>
						<plugin>
							<artifactId>plantuml-generator-maven-plugin</artifactId>
							<groupId>de.elnarion.maven</groupId>
							<version>${plantuml-generator-maven-plugin.version}</version>
							<executions>
								<!-- generation fichier interpretable par
								plantuml -->
								<execution>
									<id>generate-diagram-puml</id>
									<goals>
										<goal>generate</goal>
									</goals>
									<phase>generate-test-sources</phase>
									<configuration>
										<outputDirectory>
											${project.build.directory}/../../readme</outputDirectory>
										<outputFilename>
											${project.name}-entities.puml</outputFilename>
										<hideMethods>true</hideMethods>
										<scanPackages>
											<scanPackage>org.rudi</scanPackage>
										</scanPackages>
										<whitelistRegexp>.*\.entity\..*</whitelistRegexp> <!--
										seulement les packages org.rudi.*entity* -->
										<addJPAAnnotations>true</addJPAAnnotations>
									</configuration>
								</execution>
							</executions>
						</plugin>

						<plugin>
							<!-- generation de l'image associée au PUML-->
							<groupId>com.github.davidmoten</groupId>
							<artifactId>plantuml-maven-plugin</artifactId>
							<version>${plantuml-maven-plugin.version}</version>
							<executions>
								<execution>
									<id>generate-diagram-puml-png</id>
									<phase>generate-test-sources</phase>
									<goals>
										<goal>generate</goal>
									</goals>
									<configuration>
										<sources>
											<!-- correspond au repertoire de
											sortie de generate-diagram-puml -->
											<directory>
												${project.build.directory}/../../readme/</directory>
											<includes>
												<include>*.puml</include>
											</includes>
										</sources>
										<outputDirectory>
											${project.build.directory}/../../readme/</outputDirectory>
										<formats>
											<format>png</format>
